    "type_alias_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "type"
        },
        {
          "type": "SYMBOL",
          "name": "alias_name"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "type"
        }
      ]
    },
    "constant_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",

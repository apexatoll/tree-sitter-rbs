{
  "name": "rbs",
  "rules": {
    "program": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "declaration"
      }
    },
    "_alias": {
      "type": "PATTERN",
      "value": "[a-z]\\w*"
    },
    "_constant": {
      "type": "PATTERN",
      "value": "[A-Z]\\w*"
    },
    "_delimited_symbol": {
      "type": "PATTERN",
      "value": ":[\"']@?_*[A-Za-z]\\w*[\"']"
    },
    "_global": {
      "type": "PATTERN",
      "value": "\\$[a-zA-Z]\\w+"
    },
    "_integer": {
      "type": "PATTERN",
      "value": "[0-9][0-9_]*"
    },
    "_interface": {
      "type": "PATTERN",
      "value": "_[A-Z]\\w*"
    },
    "_scope": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "::"
      }
    },
    "_symbol": {
      "type": "PATTERN",
      "value": ":@?_*[A-Za-z]\\w*"
    },
    "declaration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "class_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "module_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "interface_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "type_alias_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "constant_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "global_declaration"
        }
      ]
    },
    "class_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "class"
        },
        {
          "type": "SYMBOL",
          "name": "class_name"
        },
        {
          "type": "STRING",
          "value": "end"
        }
      ]
    },
    "module_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "module"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "class_name"
          },
          "named": true,
          "value": "module_name"
        },
        {
          "type": "STRING",
          "value": "end"
        }
      ]
    },
    "interface_declaration": {
      "type": "SEQ",
      "members": [

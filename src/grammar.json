            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_interface"
        }
      ]
    },
    "namespace": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_scope"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "namespace"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "SYMBOL",
              "name": "_constant"
            },
            {
              "type": "SYMBOL",
              "name": "_scope"
            }
          ]
        }
      ]
    },
    "type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "builtin_type"

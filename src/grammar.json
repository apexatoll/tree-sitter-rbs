    "string_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "'.*'"
        },
        {
          "type": "PATTERN",
          "value": "\".*\""
        }
      ]
    },
    "symbol_literal": {
      "type": "CHOICE",
      "members": [
